{
    "server": {
        "accessLog": "/host/access.log",
        "port": 80,
        "workers": 10,
        "maxSockets": 10000,
        "deadBackendTTL": 30,
        "tcpTimeout": 30,
        "retryOnError": {{ retry_on_error }},
        "deadBackendOn500": false,
        "httpKeepAlive": false
    },
    "driver": ["redis://{{ redis_host }}:{{ redis_port }}"]
}
