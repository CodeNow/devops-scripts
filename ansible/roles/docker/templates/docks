DOCKER_OPTS="-H=unix:///var/run/docker.sock -H=0.0.0.0:4242 --tlsverify --tlscacert=/etc/ssl/docker/ca.pem --tlscert=/etc/ssl/docker/cert.pem --tlskey=/etc/ssl/docker/key.pem -g /docker --insecure-registry registry.runnable.com --icc=false --dns={{ charon_host | default(hostvars[groups['charon'][0]]['ansible_default_ipv4']['address'])}} --dns=8.8.8.8"
