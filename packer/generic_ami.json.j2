{
    "variables": {
        "aws_access_key": "{{ aws_access_key }}",
            "aws_secret_key": "{{ aws_secret_key }}"
    },
        "builders": [{
            "type": "amazon-ebs",
            "access_key": "{{user `aws_access_key`}}",
            "secret_key": "{{user `aws_secret_key`}}",
            "region": "us-west-2",
            "source_ami": "{{ base_ami }}",
            "instance_type": "t2.micro",
            "ssh_username": "ubuntu",
            "ami_name": "packer-{{ git_branch }}-generic {{timestamp}}"
        }]
}
